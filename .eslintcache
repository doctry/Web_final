menu\">\r\n                <FaIcons.FaBars onClick={ clickSidebar }/>\r\n            </Link>\r\n        </div>\r\n    \r\n        <nav className={ sidebar ? \"nav-menu active\" : \"nav-menu\"}>\r\n            <ul className=\"nav-menu-items\" onClick={ clickSidebar }>\r\n                { NavbarData.map((item, index) => {\r\n                    return (\r\n                        <li key={index} className={ item.cName }>\r\n                            <Link to={ item.path }>\r\n                                { item.icon }\r\n                                <span>{ item.title }</span>\r\n                            </Link>\r\n                        </li>\r\n                    );\r\n                })}\r\n            </ul>\r\n        </nav>\r\n\r\n    </IconContext.Provider>\r\n    );\r\n}\r\n\r\nexport default Navbar_in","C:\\Users\\User\\Desktop\\109-1WebProgramming\\Web_final\\src\\in\\components\\home\\Announce.js",["113","114","115"],"import React, { useState } from \"react\";\r\nimport WebPage_1 from \"./WebPage_1\";\t// 經費補助\r\nimport WebPage_2 from \"./WebPage_2\";\t// 社團資訊\r\n\r\n\r\nfunction Announce() {\r\n\tconst [showPage1, setShowPage1] = useState(true);\r\n\tconst [showPage0, setShowPage0] = useState(false);\r\n\r\n\tconst show_page1 = () => {\r\n\t\tsetShowPage1(true);\r\n\t\tsetShowPage0(false);\r\n\t}\r\n\r\n\tconst show_page2 = () => {\r\n\t\tsetShowPage1(false);\r\n\t\tsetShowPage0(true);\r\n\t}\r\n\r\n\tconst show_page3 = () => {\r\n\t\tsetShowPage1(false);\r\n\t\tsetShowPage0(false);\r\n\t}\r\n\t\r\n\treturn (\r\n\t\t<div className=\"place_Announce\">\r\n\t\t\t<h1 className=\"Announce_h1\">\r\n\t\t\t\t<button className=\"Announce_button\" onClick={show_page1}>經費補助</button> \r\n\t\t\t\t<button className=\"Announce_button\" onClick={show_page2}>社團資訊</button> \r\n\t\t\t\t<button className=\"Announce_button\" onClick={show_page3}>我想一下</button>\r\n\t\t\t</h1>\r\n\t\t\t{\r\n\t\t\t\tshowPage1? <WebPage_1 /> : showPage0 ? <WebPage_2 /> : <WebPage_1 />\