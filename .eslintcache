endar\n        plugins={[ dayGridPlugin, interactionPlugin ]}\n        initialView=\"dayGridMonth\"\n        events={events}\n        dateClick={() => {props.onDateClick()}}\n      />\n    </div>\n  );\n}\n\nexport default Calendar;\n","/home/jian-han/Desktop/Web_final/src/in/components/schedule/Todolist.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/home/Board1.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/home/Weblink.js",["138"],"import React, {useState} from \"react\";\nimport links from \"./links\";\nimport ShowLinks from \"./ShowLinks\";\nimport Add_link from \"./Add_link\";\n\n\n\nfunction Weblink() {\n\tconst [linkBoard, setLinkBoard] = useState(links);\n\tconst [show, setShow] = useState(true);\n\n\tconst click_whichshow = () => {\n\t\tsetShow(!show);\n\t}\n\n\tconst click_img = (id) => {\n\t\tsetLinkBoard(linkBoard.filter(item => item.id !== id));\n\t}\n\n\tconst last_id = () => {\n\t\tif (linkBoard.length === 0) \n\t\t\treturn \"0\"\n\t\telse \n\t\t\treturn linkBoard[linkBoard.length - 1].id\n\t}\n\n\tconst add_newlink = (newLink) => {\n\t\tlet newlinks = linkBoard;\n\t\tnewlinks.push(newLink)\n\t\tsetLinkBoard(newlinks)\n\t\tclick_whichshow()\n\t}\n\n\treturn (\n\t\t<div className=\"place_Weblink\">\n\t\t\t<h1 className=\"Weblink_h1\">書籤列</h1>\n\t\t\t{show? \n\t\t\t\t<h1 className=\"Weblink_h2\" onClick={click_whichshow}>增加連結</h1> : \n\t\t\t\t<h1 className=\"Weblink_h2\" onClick={click_whichshow}>返回書籤列</h1>\n\t\t\t}\n\t\t\t{\n\t\t\t\tshow? <ShowLinks linkboard={linkBoard} click_img={click_img}/> : <Add_link last_id={last_id} add_link={add_newlink}/>\n\t\t\t}\n\t\t</div>\n\t);\n}\n\nexport default Weblink;","/home/jian-han/Desktop/Web_final/src/in/components/home/Board0.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/home/Announce.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/CreateTask/CreateTask.js",["139"],"import { getElementError } from \"@testing-library/react\";\nimport React, { useState } from \"react\";\nimport \"./CreateTask.css\";\n\nfunction CreateTask(props) {\n  const [title, setTitle] = useState(\"\");\n  const [type, setType] = useState(0); // enum\n  const [description, setDescription] = useState(\"\");\n  const [isImportant, setIsImportant] = useState(false);\n\n  const ClickEnter = () => {\n    let inputPass = true;\n    if (title === \"\") {\n      alert(\"請輸入待辦事項\");\n      inputPass = false;\n      console.log(\"FAQ\");\n    } else if (type === 0) {\n      alert(\"請選取類別\");\n      inputPass = false;\n      console.log(\"FAQ\");\n    }\n    if (inputPass) {\n      let task = {\n        title: title,\n        deadline: props.deadline, // yyyy-mm-dd\n        type: type, // enum\n        description: description,\n        isImportant: isImportant,\n      };\n      // take the task Details from here\n      // also let inpattask in Schedule.js become false\n      props.getNewTask(task);\n    }\n  };\n\n  return (\n    <div className=\"placeright\">\n      <ul className=\"create_task__list\">\n        <li key=\"0\" className=\"create_item__item\">\n          <h1 className=\"create_item-detail\">待辦事項</h1>\n          <input\n            type=\"text\"\n            onChange={(e) => {\n              setTitle(e.target.value);\n            }}\n          />\n        </li>\n        <li key=\"1\" className=\"create_item__item\">\n          <h1 className=\"create_item-detail\">{props.deadline}</h1>\n        </li>\n        <li key=\"2\" className=\"create_item__item\">\n          <h1 className=\"create_item-detail\">類別</h1>\n          <label>租借場地</label>\n          <input\n            type=\"radio\"\n            placeholder={props.inner_text}\n            onChange={() => {\n              setType(1);\n            }}\n            checked={type === 1}\n          />\n          <label>其他</label>\n          <input\n            type=\"radio\"\n            placeholder={props.inner_text}\n            onChange={() => {\n              setType(2);\n            }}\n            checked={type === 2}\n          />\n        </li>\n        <li key=\"3\" className=\"create_item__item\">\n          <h1 className=\"create_item-detail\">敘述</h1>\n          <input\n            type=\"text\"\n            onChange={(e) => {\n              setDescription(e.target.value);\n            }}\n          />\n        </li>\n        <li key=\"4\" className=\"create_item__item\">\n          <h1 className=\"create_item-detail\">重要事項</h1>\n          <input\n            type=\"checkbox\"\n            onChange={() => {\n              setIsImportant(!isImportant);\n            }}\n            checked={isImportant}\n          />\n        </li>\n        <button onClick={ClickEnter}>送出</button>\n        <button onClick={props.cancel}>取消</button>\n      </ul>\n    </div>\n  );\n}\n\nexport default CreateTask;\n","/home/jian-han/Desktop/Web_final/src/in/components/NavbarData.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/schedule/events.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/home/links.js",[],"/home/jian-han/Desktop/Web_final/src/in/components/home/ShowLinks.js",["140"],"import React from \"react\";\nimport X from \"./img/x.png\";\n\n\nfunction ShowLints(props) {\n\treturn (\n\t\t<div className=\"showlink\">\n\t\t\t<ul className=\"Weblink_ul\">\n\t\t\t\t{props.linkboard.map((item, index) => {\n\t\t\t\t\treturn (\n\t\t\t\t\t\t<li key={index} className=\"Weblink_li\">\n\t\t\t\t\t\t\t<a href={item.href} target=\"_blank\" className=\"Weblink_a\">{item.title}</a>\n\t\t\t\t\t\t\t<img src={X} alt=\"Delete\" className=\"Weblink-x\" onClick={() => props.click_img(item.id)} />\n\t\t\t\t\t\t</li>\n\t\t\t\t\t)\n\t\t\t\t})}\n\t\t\t</ul>\n\t\t</div>\n\t);\n}\n\nexport default ShowLints;","/home/jian-han/Desktop/Web_final/src/in/components/home/WebPage.js",["141","142"],"function WebPage(props) {\n\treturn (\n\t\t<div>\n\t\t\t{console.log(props.board)}\n\t\t\t<h1 className=\"WebPage_h1\">前往: <a href={props.url} target=\"_blank\" className=\"WebPage_a1\">{